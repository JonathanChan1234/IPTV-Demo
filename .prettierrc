{
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "endOfLine": "auto",
    "jsxBracketSameLine": true,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "packageManager": "npm",
    "printWidth": 1000,
    "semi": true,
    "tabWidth": 4,
    "useTabs": false,
    "overrides": [
        {
            "files": ["*.ts", "*.tsx"],
            "options": {
                "parser": "babel-ts"
            }
        }
    ]
}
